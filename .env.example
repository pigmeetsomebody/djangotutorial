# Django 设置
DEBUG=True
SECRET_KEY=your-secret-key-here
ALLOWED_HOSTS=localhost,127.0.0.1

# 数据库设置
DATABASE_URL=sqlite:///db.sqlite3

# JWT 设置
JWT_SECRET_KEY=your-jwt-secret-key-here
JWT_ACCESS_TOKEN_LIFETIME=5
JWT_REFRESH_TOKEN_LIFETIME=1

# Cookie 设置
AUTH_COOKIE_NAME=access_token
AUTH_COOKIE_REFRESH_NAME=refresh_token
AUTH_COOKIE_SECURE=True
AUTH_COOKIE_HTTP_ONLY=True
AUTH_COOKIE_SAMESITE=Lax
AUTH_COOKIE_DOMAIN=
AUTH_COOKIE_PATH=/

# 短信服务设置（示例）
SMS_API_KEY=your-sms-api-key
SMS_API_SECRET=your-sms-api-secret
SMS_SIGN_NAME=your-sign-name
SMS_TEMPLATE_CODE=your-template-code

# 日志设置
LOG_LEVEL=INFO
LOG_FILE=logs/django.log





# 阿里云OSS设置
ALIBABA_CLOUD_REGION_ID=cn-hangzhou
OSS_ENDPOINT=oss-cn-hangzhou.aliyuncs.com
OSS_BUCKET_NAME=home-memory-image-storage
OSS_URL_PREFIX=https://home-memory-image-storage.oss-cn-hangzhou.aliyuncs.com